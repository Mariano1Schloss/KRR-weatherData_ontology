<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#"
     xml:base="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral -->

    <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral">
        <owl:equivalentClass rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#describedByPhenomena -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#describedByPhenomena">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#WeatherObservation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#WeatherPhenomena"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#hasWeatherProperty -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#hasWeatherProperty">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#WeatherObservation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#WeatherProperty"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasHumidity -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasHumidity">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#Humidity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>measured in %</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasPrecipitation -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasPrecipitation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#Precipitation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>measured in mm of rainfall</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasPressure -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasPressure">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#Pressure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>measured in Pascal</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasTemperature -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasTemperature">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#Temperature"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>measured in Celsius degrees</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasWind -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasWind">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#Wind"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>measured in km/h</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#ColdWave -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#ColdWave">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#ColdPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasTemperature"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">5.0</xsd:maxInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A spell of cold weather over a wide area.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#Drought -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#Drought">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#HotPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasPrecipitation"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">0.0</xsd:maxInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasTemperature"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">38.0</xsd:minInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A period of abnormally hot weather and no precipitation.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#HeatBurst -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#HeatBurst">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#HotPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasPrecipitation"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">10.0</xsd:maxInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasTemperature"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">38.0</xsd:minInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Rare atmospheric phenomenon characterized by a sudden, localized increase in air temperature near the Earth&apos;s surface</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#HeavyRain -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#HeavyRain">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#RainPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasPrecipitation"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">40.0</xsd:minInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A lot of rain.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#HotPhenomena -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#HotPhenomena">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#WeatherPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasTemperature"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">20.0</xsd:minInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Phenomena describing hot temperatures.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#LightRain -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#LightRain">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#RainPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasPrecipitation"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">20.0</xsd:minInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A little rain.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#WeatherObservation -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#WeatherObservation"/>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#Wind -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/WeatherPhenomena.owl#Wind">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#WeatherProperty"/>
        <rdfs:comment xml:lang="en">Speed of the wind.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#Blizzard -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#Blizzard">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#ColdPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasTemperature"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">0.0</xsd:maxInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasWind"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">85.0</xsd:minInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Severe snowstorm with high winds.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#ColdPhenomena -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#ColdPhenomena">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#WeatherPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasTemperature"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">20.0</xsd:maxExclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Phenomena describing cold temperatures.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#DustStorm -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#DustStorm">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#WindPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasHumidity"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">25.0</xsd:maxInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A strong, turbulent wind which carries clouds of fine dust, soil, and sand over a large area.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#Flood -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#Flood">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#RainPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasPrecipitation"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">100.0</xsd:minInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Heavy rain that led to flooding.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#Humidity -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#Humidity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#WeatherProperty"/>
        <rdfs:comment xml:lang="en">Humidity in the air.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#Hurricane -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#Hurricane">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#WindPhenomena"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasPrecipitation"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#hasWind"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">70.0</xsd:minInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A storm with a violent wind, in particular a tropical cyclone in the Caribbean.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#IceStorm -->

    <owl:Class rdf:about="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#IceStorm">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/estebanvivanco/ontologies/2022/4/untitled-ontology-10#ColdPhenomena"/>
